<%= form_for @school, url: {action: "create"} do |form|%>
    <p>
      <%= form.label :name %><br/>
      <%= form.text_field :name %>
    </p>
    <p>
      <%= form.label :description %><br/>
      <%= form.text_area :description, size: "60x12" %>
    </p>
    <p>
      <%= form.submit "Create" %>
    </p>
<% end %>